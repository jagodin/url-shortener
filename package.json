{
    "name": "remix-app-template",
    "private": true,
    "description": "Remix starter kit for the Serverless Framework",
    "license": "MIT",
    "sideEffects": false,
    "scripts": {
        "prebuild": "rimraf ./server/build ./public/static",
        "build": "remix build",
        "predeploy": "npm run build",
        "deploy": "sls deploy",
        "dev": "remix dev",
        "format": "prettier --write .",
        "postinstall": "remix setup node",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
        "lint-fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
        "typecheck": "tsc -b"
    },
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@mui/lab": "^5.0.0-alpha.85",
        "@mui/material": "^5.8.3",
        "@prisma/client": "^3.15.1",
        "@remix-run/architect": "^1.1.1",
        "@remix-run/react": "^1.1.1",
        "prisma": "^3.15.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "remix": "^1.1.1"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.1.1",
        "@remix-run/eslint-config": "^1.3.2",
        "@remix-run/serve": "^1.1.1",
        "@types/eslint": "^8.4.3",
        "@types/react": "^17.0.24",
        "@types/react-dom": "^17.0.9",
        "aws-sdk": "^2.1048.0",
        "esbuild": "^0.14.9",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "patch-package": "^6.4.7",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "serverless": "^2.70.0",
        "serverless-esbuild": "^1.23.2",
        "serverless-s3-sync": "^1.17.3",
        "ts-node": "^10.8.1",
        "typescript": "^4.1.2"
    },
    "engines": {
        "node": ">=14"
    }
}
